{
  "name": "webpage-tunnel",
  "version": "1.3.0",
  "description": "A secure and elegant cross-iframe API communication library based on `postMessage`.",
  "main": "dist/webpage-tunnel.cjs.js",
  "module": "dist/webpage-tunnel.esm.js",
  "types": "dist/index.d.ts",
  "unpkg": "dist/webpage-tunnel.umd.js",
  "jsdelivr": "dist/webpage-tunnel.umd.js",
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "rollup -c",
    "dev": "rollup -c -w",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "format": "biome format --write .",
    "lint": "biome lint .",
    "check": "biome check --write .",
    "demo": "npm run build && cp dist/webpage-tunnel.esm.js demo/static/webpage-tunnel.js && node demo/server.js",
    "prepublishOnly": "npm run check && npm run test && npm run build"
  },
  "keywords": [
    "iframe",
    "postMessage",
    "cross-origin",
    "communication",
    "webpage",
    "tunnel",
    "api"
  ],
  "author": "",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/parksben/webpage-tunnel.git"
  },
  "bugs": {
    "url": "https://github.com/parksben/webpage-tunnel/issues"
  },
  "homepage": "https://github.com/parksben/webpage-tunnel#readme",
  "devDependencies": {
    "@biomejs/biome": "^1.9.0",
    "@rollup/plugin-commonjs": "^28.0.1",
    "@rollup/plugin-node-resolve": "^15.3.0",
    "@rollup/plugin-typescript": "^12.1.1",
    "@types/jest": "^29.5.14",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "rollup": "^4.27.4",
    "ts-jest": "^29.2.5",
    "tslib": "^2.8.1",
    "typescript": "^5.7.2"
  },
  "dependencies": {
    "nanoid": "^5.1.6"
  }
}
